                                <option selected>Escolher...</option>
                                <option value="SP">São Paulo</option>
                                <option value="RJ">Rio de Janeiro</option>
                                <!-- Adicione mais opções conforme necessário -->
                            </select>
                        </div>
                        <div class="col-md-6">
                            <label for="cidade" class="form-label">Cidade</label>
                            <input type="text" class="form-control" id="cidade" placeholder="Digite a cidade">
                        </div>
                    </div>

                    <div class="row mb-3">
                        <div class="col-md-6">
                            <label for="tipoCliente" class="form-label">Tipo de Cliente</label>
                            <input type="text" class="form-control" id="tipoCliente" placeholder="Digite o tipo de cliente">
                        </div>
                        <div class="col-md-6">
                            <label for="tipoAtendimento" class="form-label">Tipo de Atendimento</label>
                            <input type="text" class="form-control" id="tipoAtendimento" placeholder="Digite o tipo de atendimento">
                        </div>
                    </div>

                    <div class="row mb-3">
                        <div class="col-md-6">
                            <label for="tipoMeio" class="form-label">Tipo de Meio</label>
                            <input type="text" class="form-control" id="tipoMeio" placeholder="Digite o tipo de meio">
                        </div>
                        <div class="col-md-6">
                            <label for="tipoTecnologia" class="form-label">Tipo de Tecnologia</label>
                            <input type="text" class="form-control" id="tipoTecnologia" placeholder="Digite o tipo de tecnologia">
