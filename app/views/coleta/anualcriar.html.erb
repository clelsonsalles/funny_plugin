<div class="container mt-4">
  <div class="row">
    <div class="col-md-3">
      <div class="card">
        <div class="card-body">
          <h5 class="card-title">Box 1</h5>
          <%= form_for @coleta, url: coleta_anualinserir_path do |f| %>
              <%= f.hidden_field :id_projeto, :value => @projeto.id %>
              <%= f.hidden_field :tipoColeta, :value => "estacao" %>
                    <div class="box-title">Estação</div>
                    <div class="box-description">Coleta de dados de acessos:
                    <p> <%= f.select :mes, [['Janeiro', 1], ['Fevereiro', 2], ['Março', 3], ['Abril', 4],  ['Maio', 5],  ['Junho', 6],  ['Julho', 7],  ['Agosto', 8],  ['Setembro', 9],  ['Outubro', 10],  ['Novembro', 11],  ['Dezembro', 12] ], class: "selecao" %></p>
                    </div>
                     <%= f.submit "Criar coleta", class:" icon icon-add float-right" %>
              <% end %>
        </div>
      </div>
    </div>
